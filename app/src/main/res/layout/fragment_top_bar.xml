<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context="presentation.TopBarFragment"
                                                   android:background="@drawable/topbar_background">
    <ImageButton
            android:src="@drawable/ic_mypage"
            android:layout_width="40dp"
            android:layout_height="40dp" android:id="@+id/myPageBtn"
            android:scaleType="fitCenter"
            android:background="@color/color_transparent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="28dp"
            android:padding="4dp" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/toMe_Text" android:layout_marginTop="4dp"/>
    <ImageButton
            android:src="@drawable/ic_mission"
            android:layout_width="40dp"
            android:layout_height="40dp" android:id="@+id/missionBtn"
            android:scaleType="fitCenter"
            android:background="@color/color_transparent"
            app:layout_constraintTop_toTopOf="@+id/myPageBtn"
            app:layout_constraintBottom_toBottomOf="@+id/myPageBtn"
            app:layout_constraintVertical_bias="0.0" android:padding="4dp"
            app:layout_constraintEnd_toStartOf="@+id/myPageBtn" android:layout_marginEnd="8dp"/>
    <TextView
            android:text="@string/toMe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/toMe_Text"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="28dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.467" android:textSize="12sp" android:fontFamily="@font/ydestreetl"
            android:textColor="@color/color_font1"/>
    <TextView
            android:text="몽발바닥꼬순내님,"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/naviText"
            app:layout_constraintStart_toStartOf="@+id/toMe_Text"
            android:textSize="24sp"
            app:layout_constraintTop_toBottomOf="@+id/toMe_Text"
            android:fontFamily="@font/pretendard_medium" android:textColor="@color/color_font1"
            android:layout_marginTop="9dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"/>
</androidx.constraintlayout.widget.ConstraintLayout>