        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="presentation.mission.camera.GalleryActivity">

    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:srcCompat="@tools:sample/avatars" android:id="@+id/imageView5"
